            "role": "Project Leader"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1",
        "yiisoft/yii2": "^2.0.21",
        "npm-asset/cookieconsent": "^3.1.1"
    },
    "config": {
        "process-timeout": 1800,
